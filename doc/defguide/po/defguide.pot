msgid "specified"
msgstr ""

#.(entry)
#: xml/defguide.xml:35983 xml/defguide.xml:38964 xml/defguide.xml:38965
msgid "1"
msgstr ""

#.(entry)
#: xml/defguide.xml:35988 xml/defguide.xml:35993
msgid "0"
msgstr ""

#.(para)
#: xml/defguide.xml:35998
msgid "If a viewport area is specified (and neither a content area nor scaling is specified) and <sgmltag class=\"attribute\">scalefit</sgmltag> is explicitly <quote><literal moreinfo=\"none\">0</literal></quote>, the viewport area specification must be ignored."
msgstr ""

#.(title)
#: xml/defguide.xml:36004
msgid "Alignment"
msgstr ""

#.(para)
#: xml/defguide.xml:36005
msgid "Two alignment attributes are provided, <sgmltag class=\"attribute\">align</sgmltag> and <sgmltag class=\"attribute\">valign</sgmltag>."
msgstr ""

#.(para)
#: xml/defguide.xml:36008
msgid "If specified, <sgmltag class=\"attribute\">align</sgmltag> indicates how the content area should be aligned horizontally within the viewport area. If not specified, implementations are free to choose any default value."
msgstr ""

#.(para)
#: xml/defguide.xml:36011
msgid "If specified, <sgmltag class=\"attribute\">valign</sgmltag> indicates how the content area should be aligned vertically within the viewport area. If not specified, implementations are free to choose any default value."
msgstr ""

#.(para)
#: xml/defguide.xml:36017
msgid "If nothing is specified about the size of an image, it is rendered in a content area that is the same as its intrinsic size in a viewport area that is implementation defined:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36021
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36027
msgid "An image at its intrinsic size"
msgstr ""

#.(para)
#: xml/defguide.xml:36031
msgid "If a viewport area is specified, the image is rendered in a content area that is the same as its intrinsic size in the specified viewport area:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36035
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" width=\"6in\" depth=\"5.5in\" scalefit=\"0\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36041
msgid "An image at its intrinsic size in a viewport"
msgstr ""

#.(para)
#: xml/defguide.xml:36045
msgid "If a content area is specified, the image is scaled (possibly anamorphically) to that size and rendered in a viewport area that is implementation defined:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36050
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" contentwidth=\"4in\" contentdepth=\"3in\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36056
msgid "An image at a specified content size"
msgstr ""

#.(para)
#: xml/defguide.xml:36060
msgid "If a scaling factor is specified, the intrinsic size is scaled uniformly by that amount to obtain the content area which is rendered in a viewport area that is implementation defined:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36065
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" scale=\"300\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36071
msgid "An image scaled"
msgstr ""

#.(para)
#: xml/defguide.xml:36075
msgid "If a viewport area is specified and scaling to fit is requested, the intrinsic size is scaled (uniformly) as large as possible without extending beyond the bounds of the viewport area which is rendered as specified."
msgstr ""

#.(screen)
#: xml/defguide.xml:36081
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" width=\"6in\" depth=\"5.5in\"/&gt;\n&lt;!-- note that scalefit=\"1\" is the default in this case --&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36088
msgid "An image scaled to fit"
msgstr ""

#.(para)
#: xml/defguide.xml:36092
msgid "If the viewport area and content area are specified, the image is scaled (possibly anamorphically) to the content area size and rendered in the specified viewport area:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36097
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" width=\"6in\" depth=\"5.5in\"\n           contentwidth=\"4in\" contentdepth=\"3in\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36104
msgid "An image at a specified size in a specified viewport"
msgstr ""

#.(para)
#: xml/defguide.xml:36108
msgid "If the viewport area and a scaling factor are specified, the intrinsic size is scaled uniformly by the scaling factor amount to obtain the content area which is rendered in the specified viewport area:"
msgstr ""

#.(screen)
#: xml/defguide.xml:36113
#, no-wrap
msgid "&lt;imagedata fileref=\"image.png\" width=\"6in\" depth=\"5.5in\" scale=\"300\"/&gt;"
msgstr ""

#.(phrase)
#: xml/defguide.xml:36119
msgid "An image scaled in a specified viewport"
msgstr ""

#.(para)
#: xml/defguide.xml:36127
msgid "These elements contain imagedata: <placeholder-1/>."
msgstr ""

#.(para)
#: xml/defguide.xml:36137
msgid "<sgmltag class=\"attribute\">Align</sgmltag> specifies the horizontal alignment of the content area in the viewport area."
msgstr ""

#.(para)
#: xml/defguide.xml:36145
msgid "<sgmltag class=\"attribute\">ContentDepth</sgmltag> specifies the desired depth of the content area."
msgstr ""

#.(para)
#: xml/defguide.xml:36153
msgid "<sgmltag class=\"attribute\">ContentWidth</sgmltag> specifies the desired width of the content area."
msgstr ""

#.(para)
#: xml/defguide.xml:36161
msgid "<sgmltag class=\"attribute\">Depth</sgmltag> specifies the desired depth of the viewport area."
msgstr ""

#.(para)
#: xml/defguide.xml:36169
msgid "<sgmltag class=\"attribute\">EntityRef</sgmltag> identifies the general entity which points to the content of the image data."
msgstr ""

#.(para)
#: xml/defguide.xml:36177
msgid "<sgmltag class=\"attribute\">FileRef</sgmltag> specifies the name of the file which contains the content of the image data."
msgstr ""

#.(para)
#: xml/defguide.xml:36185
msgid "<sgmltag class=\"attribute\">Format</sgmltag> identifies the format of the image data. The <sgmltag class=\"attribute\">Format</sgmltag> must be a defined notation."
msgstr ""

#.(para)
#: xml/defguide.xml:36194
msgid "<sgmltag class=\"attribute\">Scale</sgmltag> is an integer representing a percentage scaling factor (retaining the relative dimensions of the original image). If unspecified, the value 100 (100%) is assumed."
msgstr ""

#.(para)
#: xml/defguide.xml:36204
msgid "If <sgmltag class=\"attribute\">ScaleFit</sgmltag> has the value 1 (true), then the image data is to be scaled (uniformly) to the specified width or depth. The default value of 0 (false) indicates that the image will not be scaled to fit (although it may still be scaled by the <sgmltag class=\"attribute\">Scale</sgmltag> attribute)."
msgstr ""

#.(para)
#: xml/defguide.xml:36216
msgid "<sgmltag class=\"attribute\">SrcCredit</sgmltag> contains details about the source of the image data."
msgstr ""

#.(refpurpose), (entry)
