msgstr ""

#.(para)
#: xml/defguide.xml:56492
msgid "<sgmltag>RefMiscInfo</sgmltag> is an escape hatch for additional meta-information about a reference page. It may hold copyright information, release or revision information, descriptive text for use in a print header or footer, or any other information not explicitly provided for in <sgmltag>RefMeta</sgmltag>."
msgstr ""

