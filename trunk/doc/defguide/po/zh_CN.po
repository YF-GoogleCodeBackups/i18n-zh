#. (programlisting)
#: xml/defguide.xml:9135
#, no-wrap
msgid ""
"\n"
"&lt;!ENTITY % sect1.module \"IGNORE\"&gt;                <co id=\"remvattr.1\"></co>\n"
"\n"
"&lt;!-- load DocBook --&gt;                            <co id=\"remvattr.2\"></co>\n"
"&lt;!ENTITY % DocBookDTD PUBLIC \"-//OASIS//DTD DocBook V3.1//EN\"&gt;\n"
"%DocBookDTD;\n"
"\n"
"&lt;!ENTITY % local.sect1.attrib \"\"&gt;                <co id=\"remvattr.3\"></co>\n"
"&lt;!ENTITY % sect1.role.attrib \"%role.attrib;\"&gt;    <co id=\"remvattr.4\"></co>\n"
"&lt;!ELEMENT Sect1 - O (Sect1Info?, (%sect.title.content;), (%nav.class;)*,  <co id=\"remvattr.5\"></co>\n"
"        (((%divcomponent.mix;)+, \n"
"        ((%refentry.class;)* | Sect2* | SimpleSect*))\n"
"        | (%refentry.class;)+ | Sect2+ | SimpleSect+), (%nav.class;)*)\n"
"        +(%ubiq.mix;)&gt;\n"
